{
    "compilerOptions": {
        "target": "ES5",
        "module": "ESNext",
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "#/*": ["src/*"]
        },
        "noEmit": false,
        "declaration": true,
        "emitDeclarationOnly": true,
        "declarationDir": "./dist",
        "outDir": "./dist"
    },
    "include": ["src"]
}
